.btn-info{
    color:#ffffff;
    text-shadow:0 -1px 0 rgba(0, 0, 0, 0.25);
    background-color:#FE5E00;
    background-image:-moz-linear-gradient(top, #FE5E00, #FE5E00);
    background-image:-webkit-gradient(linear, 0 0, 0 100%, from(#FE5E00), to(#FE5E00));
    background-image:-webkit-linear-gradient(top, #FE5E00, #FE5E00);
    background-image:-o-linear-gradient(top, #FE5E00, #FE5E00);
    background-image:linear-gradient(to bottom, #FE5E00, #FE5E00);
    background-repeat:repeat-x;
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#FE5E00', endColorstr='#ff2f96b4', GradientType=0);
    border-color:#FE5E00 #FE5E00 #FE5E00;
    border-color:rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    *background-color:#FE5E00;
    filter:progid:DXImageTransform.Microsoft.gradient(enabled = false);
}

.btn-info:hover,.btn-info:active,.btn-info.active,.btn-info.disabled,.btn-info[disabled]{color:#ffffff;background-color:#FE5E00;*background-color:#FE5E00;}
.btn-info:active,.btn-info.active{background-color:#FE5E00 \9;}

